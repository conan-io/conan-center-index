[MASTER]
load-plugins=linter.conanv2_transition
py-version=3.6
recursive=no
suggestion-mode=yes
unsafe-load-any-extension=no

[MESSAGES CONTROL]
disable=all
enable=conan-bad-name,
       conan-missing-name

[REPORTS]
evaluation=max(0, 0 if fatal else 10.0 - ((float(5 * error) / statement) * 10))
output-format=text
reports=no
score=no

