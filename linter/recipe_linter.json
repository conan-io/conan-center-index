{
    "problemMatcher": [
        {
            "owner": "recipe_linter_errors",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "(Error: )?(\\S+):(\\d+): \\[(E\\d+\\(\\S+\\)),\\s(.+?)\\](.+)",
                    "file": 2,
                    "line": 3,
                    "message": 6,
                    "code": 4
                }
            ]
        },
        {
            "owner": "recipe_linter_warnings",
            "severity": "warning",
            "pattern": [
                {
                    "regexp": "(Warning: )?(\\S+):(\\d+): \\[(W\\d+\\(\\S+\\)),\\s(.+?)\\](.+)",
                    "file": 2,
                    "line": 3,
                    "message": 6,
                    "code": 4
                }
            ]
        }
    ]
}
