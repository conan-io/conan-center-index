From c256416347c19e306f8a9a5f8560b3c754e01c87 Mon Sep 17 00:00:00 2001
From: Uilian Ries <>
Date: Fri, 19 Sep 2025 08:52:47 +0200
Subject: [PATCH] Avoid enforcing all linkers to use copy-dt-needed-entries

https://github.com/facebook/rocksdb/issues/13895
---
 CMakeLists.txt | 1 -
 1 file changed, 1 deletion(-)

diff --git a/CMakeLists.txt b/CMakeLists.txt
index 7c41a59..c7590f2 100644
--- a/CMakeLists.txt
+++ b/CMakeLists.txt
@@ -629,7 +629,6 @@ if(USE_FOLLY)
   add_compile_definitions(USE_FOLLY FOLLY_NO_CONFIG HAVE_CXX11_ATOMIC)
   list(APPEND THIRDPARTY_LIBS Folly::folly)
   set(FOLLY_LIBS Folly::folly)
-  set(CMAKE_EXE_LINKER_FLAGS "${CMAKE_EXE_LINKER_FLAGS} -Wl,--copy-dt-needed-entries")
 endif()
 find_package(Threads REQUIRED)

