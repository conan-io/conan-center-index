From f8b3b738ade5652e052daee3281abfd404ebffbd Mon Sep 17 00:00:00 2001
From: Tarcisio Rodrigues <tarcisio.genaro@gmail.com>
Date: Tue, 9 Jun 2020 08:37:15 -0300
Subject: [PATCH 2/4] remove from examples dependency on openssl source

---
 examples/executor/C++/CMakeLists.txt | 2 +-
 examples/ordermatch/CMakeLists.txt   | 2 +-
 examples/tradeclient/CMakeLists.txt  | 2 +-
 3 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/examples/executor/C++/CMakeLists.txt b/examples/executor/C++/CMakeLists.txt
index 9df6ddaa..6bc9ba10 100644
--- a/examples/executor/C++/CMakeLists.txt
+++ b/examples/executor/C++/CMakeLists.txt
@@ -7,7 +7,7 @@ if (WIN32)
 set (executor_NAME ${executor_NAME}_cpp)
 endif()
 
-add_executable(${executor_NAME} Application.cpp executor.cpp ${applink_SOURCE})
+add_executable(${executor_NAME} Application.cpp executor.cpp)
 
 target_include_directories(${executor_NAME} PRIVATE ${CMAKE_SOURCE_DIR}/include ${CMAKE_SOURCE_DIR}/include/quickfix ${CMAKE_SOURCE_DIR})
 
diff --git a/examples/ordermatch/CMakeLists.txt b/examples/ordermatch/CMakeLists.txt
index 10bb08e0..ae8b1751 100644
--- a/examples/ordermatch/CMakeLists.txt
+++ b/examples/ordermatch/CMakeLists.txt
@@ -2,7 +2,7 @@ if (HAVE_SSL AND WIN32)
 set(applink_SOURCE ${OPENSSL_INCLUDE_DIR}/openssl/applink.c)
 endif()
 
-add_executable(ordermatch Application.cpp Market.cpp ordermatch.cpp ${applink_SOURCE})
+add_executable(ordermatch Application.cpp Market.cpp ordermatch.cpp)
 
 target_include_directories(ordermatch PRIVATE ${CMAKE_SOURCE_DIR}/include ${CMAKE_SOURCE_DIR}/include/quickfix ${CMAKE_SOURCE_DIR})
 
diff --git a/examples/tradeclient/CMakeLists.txt b/examples/tradeclient/CMakeLists.txt
index 69836ec6..bc410d88 100644
--- a/examples/tradeclient/CMakeLists.txt
+++ b/examples/tradeclient/CMakeLists.txt
@@ -2,7 +2,7 @@ if (HAVE_SSL AND WIN32)
 set(applink_SOURCE ${OPENSSL_INCLUDE_DIR}/openssl/applink.c)
 endif()
 
-add_executable(tradeclient Application.cpp tradeclient.cpp ${applink_SOURCE})
+add_executable(tradeclient Application.cpp tradeclient.cpp)
 
 target_include_directories(tradeclient PRIVATE ${CMAKE_SOURCE_DIR}/include ${CMAKE_SOURCE_DIR}/include/quickfix ${CMAKE_SOURCE_DIR})
 
