index 00000000..040fd0a9
--- /dev/null
+++ b/CMake/EntosWidget.cmake
@@ -0,0 +1,122 @@
+set(config_xml_template [[
+ <widget xmlns="http://www.bskyb.com/ns/widgets" version="2.0">
+    <name short="com.entos.@wgt_EXECUTABLE_TARGET@" version="@wgt_VERSION@">@wgt_EXECUTABLE_TARGET@</name>
+    <icon src="icon.png" type="image/png" />
+    <content src="@widget_target@" type="application/system">
+        <platformFilters>
+            <region name="GBR" />
+        </platformFilters>
+    </content>
+    <capabilities>
+        <capability name="local-services-1" />
+        <capability name="wan-lan" />
+        <capability name="storage" />
+        <capability name="private-storage">255</capability>
+        <capability name="refresh-rate-60hz" />
+        <capability name="drm-store" />
+        <capability name="drm-type">com.widevine.alpha</capability>
+        <capability name="thunder" />
+        <capability name="sys-memory-limit">220m</capability>
+        <capability name="gpu-memory-limit">350m</capability>
+    </capabilities>
+</widget>
+]])
+
+function(add_widget_target)
+    set(option_args ALL)
+    set(one_value_args EXECUTABLE_TARGET TARGET VERSION LAUNCHER_CONTENT CONFIG_CONTENT)
+    set(multi_value_args LIBS FILES DIRECTORIES)
+    cmake_parse_arguments(PARSE_ARGV 0 wgt "${option_args}" "${one_value_args}" "${multi_value_args}")
+
+    if (wgt_ALL)
+        set(wgt_ALL ALL)
+    endif()
+
+    if (NOT wgt_TARGET)
+        set(wgt_TARGET ${wgt_EXECUTABLE_TARGET}-widget)
+    endif()
+
+    if (NOT wgt_VERSION)
+        if (NOT PROJECT_VERSION)
+            message(FATAL_ERROR "If VERSION is not specified, PROJECT_VERSION must be set")
+        endif()
+        set(wgt_VERSION ${PROJECT_VERSION})
+    endif()
+
+    set_target_properties(${wgt_EXECUTABLE_TARGET} PROPERTIES
+            INSTALL_RPATH "$ORIGIN/lib"
+            BUILD_WITH_INSTALL_RPATH ON
+    )
+
+    set(widget_name "${wgt_EXECUTABLE_TARGET}_${CMAKE_SYSTEM_PROCESSOR}_${CMAKE_BUILD_TYPE}.wgt")
+    string(TOLOWER ${widget_name} widget_name)
+
+    set(widget_dependencies
+        config.xml
+        ${wgt_EXECUTABLE_TARGET}
+        ${wgt_FILES}
+        ${wgt_DIRECTORIES}
+        ${wgt_LIBS}
+    )
+    set(widget_src_files ${wgt_FILES} ${CMAKE_CURRENT_BINARY_DIR}/config.xml)
+
+    if (wgt_LAUNCHER_CONTENT)
+        file(CONFIGURE OUTPUT launcher.sh CONTENT ${wgt_LAUNCHER_CONTENT} @ONLY)
+        list(APPEND widget_dependencies launcher.sh)
+        set(widget_target launcher.sh)
+        list(APPEND widget_src_files ${CMAKE_CURRENT_BINARY_DIR}/launcher.sh)
+    else()
+        set(widget_target ${wgt_EXECUTABLE_TARGET})
+    endif()
+
+    if (NOT wgt_CONFIG_CONTENT)
+        set(wgt_CONFIG_CONTENT ${config_xml_template})
+    endif()
+    file(CONFIGURE OUTPUT config.xml CONTENT ${wgt_CONFIG_CONTENT} @ONLY)
+
+    set(widget_dir ${CMAKE_BINARY_DIR}/${wgt_TARGET})
+    set(widget_staging ${widget_dir}/staging)
+    set(widget_bin ${widget_staging})
+    set(widget_lib ${widget_staging}/lib)
+
+    add_custom_command(OUTPUT ${widget_dir}/${widget_name}
+        COMMAND ${CMAKE_COMMAND} -E rm -rf ${widget_staging}
+        COMMAND ${CMAKE_COMMAND} -E make_directory ${widget_bin} ${widget_lib}
+        COMMAND ${CMAKE_COMMAND} -E copy ${widget_src_files} ${widget_staging}
+        COMMAND ${CMAKE_COMMAND} -E copy $<TARGET_FILE:${wgt_EXECUTABLE_TARGET}> ${widget_bin}
+        DEPENDS ${widget_dependencies}
+    )
+
+    if (wgt_LIBS)
+        add_custom_command(OUTPUT ${widget_dir}/${widget_name}
+            COMMAND ${CMAKE_COMMAND} -E copy ${wgt_LIBS} ${widget_lib}
+            APPEND
+        )
+    endif()
+
+    if (wgt_DIRECTORIES)
+        # copy_directory will copy directory contents but not the directory itself,
+        # so append the directory name to the destination path
+        foreach (dir ${wgt_DIRECTORIES})
+            cmake_path(SET dir_path ${dir})
+            cmake_path(GET dir_path STEM dir_name)
+            cmake_path(SET dest_dir_path ${widget_staging}/${dir_name} NORMALIZE)
+            add_custom_command(OUTPUT ${widget_dir}/${widget_name}
+                OUTPUT ${widget_name}
+                COMMAND ${CMAKE_COMMAND} -E copy_directory ${dir} ${dest_dir_path}
+                APPEND
+            )
+        endforeach ()
+    endif()
+
+    add_custom_command(OUTPUT ${widget_dir}/${widget_name}
+        COMMAND tar -czf ${widget_dir}/${widget_name} -C ${widget_staging} "."
+        APPEND
+    )
+
+    if (wgt_ALL)
+        set(extra_args ALL)
+    endif()
+
+    add_custom_target(${wgt_TARGET} ${extra_args} DEPENDS ${widget_dir}/${widget_name})
+endfunction()
