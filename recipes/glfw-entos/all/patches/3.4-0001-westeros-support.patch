--- /dev/null
+++ b/toolchain/rdk-toolchain-defaults.cmake
@@ -0,0 +1,70 @@
+function(get_rdk_sdk_path_or_default rdk_sdk_path)
+    # Get the RDK_SDK_PATH. The following variables have precedence:
+    # 1. First, the `RDK_SDK_PATH` CMake variable is examined and used if it is valid
+    # 2. Otherwise, the Environment Variable `RDK_SDK_PATH` is examined, and used if it is valid
+    # 3. Otherwise, the location `$HOME/rdk/dunfell-sdk` is used if it is valid
+    set(_rdk_sdk_path ${RDK_SDK_PATH})
+    if (NOT DEFINED _rdk_sdk_path)
+        if (NOT DEFINED ENV{RDK_SDK_PATH})
+            set(_rdk_sdk_path "$ENV{HOME}/rdk/kirkstone-sdk")
+        else ()
+            set(_rdk_sdk_path "$ENV{RDK_SDK_PATH}")
+        endif ()
+    endif ()
+    if (NOT EXISTS "${_rdk_sdk_path}")
+        message(FATAL_ERROR "RDK_SDK_PATH was set to the path '${_rdk_sdk_path}' which does not exist")
+    endif ()
+    set(${rdk_sdk_path} ${_rdk_sdk_path} PARENT_SCOPE)
+endfunction()
+
+function(iui_initialize_rdk_toolchain_vars)
+    set(one_value_args TARGET_TRIPLE TARGET_SYSROOT HOST_SYSROOT)
+    cmake_parse_arguments(_rdk "" "${one_value_args}" "" ${ARGN})
+    get_rdk_sdk_path_or_default(rdk_sdk_path)
+
+    # this is necessary if we are building dependencies with conan
+    set(ENV{RDK_SDK_PATH} ${rdk_sdk_path})
+
+    set(rdk_host_toolchain_path "${rdk_sdk_path}/sysroots/${_rdk_HOST_SYSROOT}")
+    set(RDK_TOOLCHAIN_BIN "${rdk_host_toolchain_path}/bin" PARENT_SCOPE)
+    set(rdk_toolchain_path "${rdk_host_toolchain_path}/usr/bin/${_rdk_TARGET_TRIPLE}")
+    set(rdk_toolchain_path_prefix "${rdk_toolchain_path}/${_rdk_TARGET_TRIPLE}")
+
+    set(CMAKE_CXX_COMPILER "${rdk_toolchain_path_prefix}-g++" PARENT_SCOPE)
+    set(CMAKE_C_COMPILER "${rdk_toolchain_path_prefix}-gcc" PARENT_SCOPE)
+    set(CMAKE_C_COMPILER_TARGET ${_rdk_TARGET_TRIPLE} PARENT_SCOPE)
+    set(CMAKE_AR "${rdk_toolchain_path_prefix}-ar" PARENT_SCOPE)
+
+    set(CMAKE_CXX_COMPILER_RANLIB "${rdk_toolchain_path_prefix}-gcc-ranlib" PARENT_SCOPE)
+    set(CMAKE_CXX_COMPILER_TARGET ${_rdk_TARGET_TRIPLE} PARENT_SCOPE)
+
+    # This is what allows us to search for Conan packages first before searching the RDK
+    list(PREPEND CMAKE_FIND_ROOT_PATH "${CMAKE_BINARY_DIR}/conan;${rdk_host_toolchain_path}")
+    set(CMAKE_FIND_ROOT_PATH ${CMAKE_FIND_ROOT_PATH} PARENT_SCOPE)
+
+    set(OE_QMAKE_PATH_EXTERNAL_HOST_BINS "${rdk_host_toolchain_path}/usr/bin" PARENT_SCOPE)
+    if (NOT _rdk_TARGET_SYSROOT)
+        set(_rdk_TARGET_SYSROOT ${_rdk_TARGET_TRIPLE})
+    endif ()
+    set(CMAKE_SYSROOT "${rdk_sdk_path}/sysroots/${_rdk_TARGET_SYSROOT}" PARENT_SCOPE)
+    # TODO (andrew) make sure these are fallbacks - ie, if a conan package exists we
+    #  want that in preference to the RDK SDK version.
+    set(ENV{PKG_CONFIG_LIBDIR} "${CMAKE_SYSROOT}/usr/lib/pkgconfig")
+    set(ENV{PKG_CONFIG_SYSROOT_DIR} "${CMAKE_SYSROOT}")
+endfunction()
+
+set(RDK ON)
+set(ENTOS ON)
+set(CMAKE_SYSTEM_NAME Linux)
+set(CMAKE_SYSTEM_PROCESSOR arm)
+
+if (NOT DEFINED CMAKE_FIND_ROOT_PATH_MODE_PACKAGE OR CMAKE_FIND_ROOT_PATH_MODE_PACKAGE STREQUAL "ONLY")
+    set(CMAKE_FIND_ROOT_PATH_MODE_PACKAGE "BOTH")
+endif ()
+if (NOT DEFINED CMAKE_FIND_ROOT_PATH_MODE_LIBRARY OR CMAKE_FIND_ROOT_PATH_MODE_LIBRARY STREQUAL "ONLY")
+    set(CMAKE_FIND_ROOT_PATH_MODE_LIBRARY "BOTH")
+endif ()
+if (NOT DEFINED CMAKE_FIND_ROOT_PATH_MODE_INCLUDE OR CMAKE_FIND_ROOT_PATH_MODE_INCLUDE STREQUAL "ONLY")
+    set(CMAKE_FIND_ROOT_PATH_MODE_INCLUDE "BOTH")
+endif ()
+set(CMAKE_FIND_ROOT_PATH_MODE_PROGRAM "NEVER")
